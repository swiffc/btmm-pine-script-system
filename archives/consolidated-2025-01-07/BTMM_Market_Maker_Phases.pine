    overall_score = 0
    if win_rate >= 50: overall_score += 1
    if not na(profit_factor) and profit_factor >= 1.5: overall_score += 1
    if max_drawdown <= 10: overall_score += 1
    if total_return >= 10: overall_score += 1
    if total_trades >= 20: overall_score += 1
    
