    strength += require_ema_stack and (bull_stack or bear_stack) ? 20 : 0
    strength += require_volume_conf and volume_ratio > volume_spike_threshold ? 15 : 0
    strength += (london_session or ny_session) ? 10 : 0
    math.min(strength, 100)

