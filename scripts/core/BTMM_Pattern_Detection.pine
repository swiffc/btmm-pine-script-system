// === RISK MANAGEMENT ===
risk_group = "ğŸ›¡ï¸ Risk Management"
stop_loss_atr_mult = input.float(2.0, "Stop Loss ATR Multiplier", minval=0.5, maxval=5.0, group=risk_group)
